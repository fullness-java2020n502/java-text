package text2;

public class SampleDriver {
	public static void main(String[] args) {
		// Carクラスを呼び出す
		Car car = new Car("ポルシェ","青",100,60);
		// フィールドの更新
		Car car2 = new Car();
		car2.name = "ポルシェ";
		car2.color = "青";
		car2.fuel = 100;
		car2.speed = 60;
		// フィールドの参照
		//String name = car.name;
		//String color = car.color;
		//int fuel = car.fuel;
		System.out.println(car.name);
		System.out.println(car.color);
		System.out.println(car.fuel);
		//int result = car.run();
		// メソッドの呼び出し
		car.speedUp(100);
		int result = car.run();
		System.out.println(result);
	}
}
