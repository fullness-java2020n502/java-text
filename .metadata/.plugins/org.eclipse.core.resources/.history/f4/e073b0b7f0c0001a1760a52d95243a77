package practice11;
/**
 * 演習11 税クラス
 * @author imagepit
 */
public class Tax {
	// スタティックフィールド
	static double rate; // その後に処理
	// スタティックイニシャライザ
	static {
		System.out.println("JVM読み込み後");
		rate = 0.08; // JMV呼び込み時（一番早い）
	}
	// スタティックメソッド
	static int getTaxPrice(int price) {
		return (int)(price * rate);
	}
}
